<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" fx:id="file" prefHeight="478.0" prefWidth="760.0" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="Application.Methods.Controller">
   <Pane prefHeight="508.0" prefWidth="720.0" GridPane.columnIndex="2" fx:id="start">

      <Button fx:id="mainButton" alignment="CENTER" ellipsisString="" layoutX="263.0" layoutY="343.0"
              onAction="#buttonClicked" onKeyPressed="#buttonClicked" opacity="0.94" prefHeight="46.0" prefWidth="195.0"
              text="Дізнатися діагноз" textOverrun="WORD_ELLIPSIS">
         <rotationAxis>
            <Point3D z="2.0"/>
         </rotationAxis>
         <font>
            <Font name="Microsoft Yi Baiti" size="17.0"/>
         </font>
      </Button>
      <Label fx:id="HelloText" layoutX="188.0" layoutY="148.0" prefHeight="84.0" prefWidth="455.0" text="Опишіть ваші симптоми">
         <font>
            <Font name="Microsoft YaHei Light" size="29.0"/>
         </font>
      </Label>
      <TextField fx:id="queryField" layoutX="105.0" layoutY="262.0" prefHeight="39.0" prefWidth="517.0"
                 promptText="Набридли гачімучі">
         <font>
            <Font size="17.0"/>
         </font>
      </TextField>
      <Label fx:id="BigText" layoutX="130.0" layoutY="65.0" prefHeight="72.0" prefWidth="470.0"
             text="Вітаю! Я Ваш медичний помічник!">
         <font>
            <Font name="Microsoft JhengHei" size="30.0"/>
         </font>
      </Label>
   </Pane>
   <Pane prefHeight="508.0" prefWidth="720.0" GridPane.columnIndex="2" fx:id="end" >
   <Button fx:id="backBtn" alignment="CENTER" ellipsisString="" layoutX="250.0" layoutY="430"
           onAction="#goBack" onKeyPressed="#goBack" opacity="0.94" prefHeight="46.0" prefWidth="195.0"
           text="Повернутися" textOverrun="WORD_ELLIPSIS">
      <rotationAxis>
         <Point3D z="2.0"/>
      </rotationAxis>
      <font>
         <Font name="Microsoft Yi Baiti" size="17.0"/>
      </font>
   </Button>
      <Button fx:id="SortByPricebtn" alignment="CENTER" ellipsisString="" prefHeight="30" layoutX="350" layoutY="390"
                    opacity="0.94"
                    text="Відсортувати за ціною" textOverrun="WORD_ELLIPSIS">
      <rotationAxis>
         <Point3D z="2.0"/>
      </rotationAxis>
      <font>
         <Font name="Microsoft Yi Baiti" size="15.0"/>
      </font>
   </Button>
      <Button fx:id="SortByNamebtn" alignment="CENTER" ellipsisString="" prefHeight="30" layoutX="150.0" layoutY="390"
                    opacity="0.94"
                    text="Відсортувати за ім'ям" textOverrun="WORD_ELLIPSIS">
      <rotationAxis>
         <Point3D z="2.0"/>
      </rotationAxis>
      <font>
         <Font name="Microsoft Yi Baiti" size="15.0"/>
      </font>
   </Button>
   </Pane>
</GridPane>
